	cd Rendering          ; make clean -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd SceneGraph         ; make clean -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Shaders            ; make clean -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd SharedArrays       ; make clean -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Sorting            ; make clean -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
