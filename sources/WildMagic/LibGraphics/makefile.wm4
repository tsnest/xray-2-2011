build :
	@mkdir -p ../SDK/Include
	@mkdir -p ../SDK/Library/$(CFG)
	@mkdir -p $(CFG)
	@cp -fp *.h ../SDK/Include
	cd Collision          ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Controllers        ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Curves             ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Detail             ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Effects            ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Mathematics        ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd ObjectSystem       ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Rendering          ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd SceneGraph         ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Shaders            ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd SharedArrays       ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Sorting            ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Surfaces           ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)
	cd Terrain            ; make -f ../makeprj.wm4 CFG=$(CFG) SYS=$(SYS)

