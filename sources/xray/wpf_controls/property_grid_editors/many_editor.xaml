<ResourceDictionary x:Class="xray.editor.wpf_controls.property_grid_editors.many_editor"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<DataTemplate x:Key="many_editor">
		<DockPanel>
			<Button Click="Button_Clicked" DockPanel.Dock="Right">
				<Rectangle Margin="2" Width="8" Height="8" StrokeThickness="2" Stroke="Gray" VerticalAlignment="Center" HorizontalAlignment="Center" >
					<Rectangle.RenderTransform>
						<RotateTransform Angle="45" CenterX="4" CenterY="4"/>
					</Rectangle.RenderTransform>
				</Rectangle>
				<Button.ToolTip>
					Reset All Values
				</Button.ToolTip>
			</Button>
			<TextBlock Text="&lt;many&gt;" VerticalAlignment="Center"/>
		</DockPanel>
	</DataTemplate>
</ResourceDictionary>